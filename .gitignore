# Node artifact files
node_modules/
dist/

# Composer artifact files
vendor/

# Compiled Java class files
*.class

# Compiled Python bytecode
*.py[cod]

# Log files
log/
*.log

# Package files
*.jar

# Maven
target/
dist/

# Unit test reports
TEST*.xml

# Generated by MacOS
.DS_Store

# Generated by Windows
Thumbs.db
.bat
*.bat

# Applications
*.app
*.exe
*.war

# Large media files
*.mp4
*.tiff
*.avi
*.flv
*.mov
*.wmv
.bat
*.bat

# IDEsT text editor
.vscode
.vscode/
*.sublime-project
*.sublime-workspace
.idea/


# Exclusive theme files
cache/
temp/
*.env
sitemaps/
sitemap.xml
sitemap/*.xml

# Ignore everything inside uploads/images
uploads/images/**

!example.env

# Re-include base directories
!uploads/images/
!uploads/images/brand/
!uploads/images/users/

# Allow specific files
!uploads/images/**/preview_img.jpg
!uploads/images/brand/**
!uploads/images/users/preview_img.jpg
